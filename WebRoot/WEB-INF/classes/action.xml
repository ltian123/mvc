<?xml version="1.0" encoding="UTF-8"?>
<action-mappings>

	<action path="/regist" type="action.UserAction" parameter="regist">
		<forward name="success" path="/pages/login.jsp" redirect="true"></forward>
		<forward name="fail" path="/pages/regist.jsp"></forward>
	</action>
	
	
	<action path="/login" type="action.UserAction" parameter="login">
		<forward name="success" path="/pages/success.jsp"></forward>
	</action>

</action-mappings>
